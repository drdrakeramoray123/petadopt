<html>
    <head>
        <%- include("../includes/head") %> 
        <title><%= animalData.breedName %>  Animal Details</title>
        <script src="/scripts/imageGallerySelector.js" defer></script>
    </head>
    <body class="bg-slate-200">
        <%- include("../includes/navbar") %>
        <!-- Details -->
        <section class="text-gray-600 body-font overflow-hidden">
            <div class="container mx-auto">
                <div class="lg:w-4/5 mx-auto flex flex-wrap">
                    <section class="pt-12 rounded-b-10xl overflow-hidden">
                        <div class="container px-4 mx-auto">
                            <div class="flex flex-wrap -mx-4">
                                <%- include("../includes/breadcrumb", {type:
                                animalData.type, breed: animalData.breedName}) %>
                                <div class="w-full lg:w-1/2 px-4 py-8">
                                    <% if (animalData.images.length > 0) { %>
                                    <%- include("../includes/image-gallery", {
                                    images: animalData.images, suffix: "public"}) %>
                                    <% } %>
                                </div>
                                <div class="w-full lg:w-1/2 px-4">
                                    <div class="w-fullmb-6">
                                        <h2
                                            class="mt-6 mb-4 text-xl md:text-2xl lg:text-3xl font-heading font-medium"
                                        >
                                            <%= animalData.breedName %>
                                        </h2>
                                        <p
                                            class="text-lg text-gray-400 text-justify"
                                        >
                                            <%= animalData.description %>
                                        </p>
                                    </div>
                                    <div class="flex flex-wrap">
                                        <span
                                            class="mb-3 text-lg w-full lg:w-1/2"
                                        >
                                            <span>Mixed Breed:</span>
                                            <span class="text-gray-400"
                                                ><%= animalData.isMixBreed.charAt(0).toUpperCase() + animalData.isMixBreed.slice(1) %>
                                            </span>
                                        </span>
                                        <span
                                            class="mb-3 text-lg w-full lg:w-1/2"
                                        >
                                            <span>Origin:</span>
                                            <span class="text-gray-400"
                                                ><%= animalData.animalOrigin %>
                                                </i>
                                            </span>
                                        </h4>
                                    </div>
                                    <div class="mb-6">
                                        <h4
                                            class="mb-3 font-heading font-medium"
                                        >
                                            <span>Color:</span>
                                            <span class="text-gray-400"
                                                >
                                                <% 
                                                if(animalData.color)
                                                    { for(let col of
                                                    animalData.color) {
                                                    %> <%=
                                                    col.charAt(0).toUpperCase()
                                                    + col.slice(1) %>
                                                    <% } %> <% } else { %>
                                                    <span class="text-gray-400">
                                                        No information
                                                    </span>
                                                    <% } %>
                                                </span
                                            >
                                        </h4>
                                    </div>
                                    <div class="flex flex-wrap -mx-2 mb-12">
                                        <div
                                            class="w-full px-2 mb-2 md:mb-0"
                                        >
                                            <a
                                                class="block py-2 px-2 leading-8 font-heading font-medium tracking-tighter text-xl text-white text-center bg-indigo-500 focus:ring-2 focus:ring-indigo-500 focus:ring-opacity-50 hover:bg-indigo-600 rounded-xl"
                                                href="/find/breed/<%= animalData.breedName %>"
                                            >
                                                Find <%= animalData.breedName %> to Adopt
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <div class="w-full px-4">
                                    <div>
                                        <h4 class="mb-6 text-2xl">
                                            More information
                                        </h4>
                                        <hr />
                                        <div class="flex flex-wrap">
                                            <details
                                                class="flex w-full lg:w-1/2 pr-6 py-4 mb-2 justify-between items-center leading-7 rounded-2xl border-2 border-gray-200 bg-white p-2"
                                            >
                                                <summary
                                                    class="text-lg font-heading font-medium rounded-md"
                                                >
                                                    Qualities
                                                </summary>
                                                <span
                                                    class="pl-6 text-justify border-t-2 border-gray-400"
                                                >
                                                    <%
                                                    if(animalData.qualities)
                                                    { for(let quality in
                                                    animalData.qualities) {
                                                    %> 
                                                    <section class="flex flex-wrap">
                                                        <span class="w-1/2">
                                                            <%=
                                                            quality.charAt(0).toUpperCase()
                                                            + quality.slice(1).replace("_", " ") %>
                                                        </span>
                                                        <span class="w-1/2">
                                                            <%= animalData.qualities[quality] %>
                                                        </span>
                                                    </section>
                                                    <% } %> <% } else { %>
                                                    <span class="text-gray-400">
                                                        No information
                                                    </span>
                                                    <% } %>
                                                </span>
                                            </details>
                                            <details
                                                class="flex w-full lg:w-1/2 pr-6 py-4 mb-2 justify-between items-center leading-7 rounded-2xl border-2 border-gray-200 bg-white p-2"
                                            >
                                                <summary
                                                    class="text-lg font-heading font-medium rounded-md"
                                                >
                                                    Physical details
                                                </summary>
                                                <span
                                                    class="pl-6 text-justify border-t-2 border-gray-400"
                                                >
                                                <%
                                                    if(animalData.physical)
                                                    { for(let phy in
                                                    animalData.physical) {
                                                    %> 
                                                    <section class="flex flex-wrap">
                                                        <span class="w-1/2">
                                                        <%=
                                                        phy.charAt(0).toUpperCase()
                                                        + phy.slice(1).replace("_", " ") %>
                                                        </span>
                                                        <span class="w-1/2">
                                                            <%= animalData.physical[phy] %>
                                                        </span>
                                                    </section>
                                                    <% } %>
                                                    <section class="flex flex-wrap">
                                                        <span class="w-1/2">
                                                        Size
                                                        </span>
                                                        <span class="w-1/2">
                                                            <%= animalData.size %>
                                                        </span>
                                                        <span class="w-1/2">
                                                        Life (Avg)
                                                        </span>
                                                        <span class="w-1/2">
                                                            <%= animalData.avg_life %> years
                                                        </span>
                                                        <span class="w-1/2">
                                                        Weight (Avg)
                                                        </span>
                                                        <span class="w-1/2">
                                                            <%= animalData.avg_weight %> Kgs
                                                        </span>
                                                        <span class="w-1/2">
                                                        Height(Avg)
                                                        </span>
                                                        <span class="w-1/2">
                                                            <%= animalData.avg_height %> inch
                                                        </span>
                                                    </section>
                                                    <% } else { %>
                                                    <span class="text-gray-400">
                                                        No information
                                                    </span>
                                                    <% } %>
                                                </span>
                                            </details>
                                        </div>
                                        <div class="flex flex-wrap">
                                            <details
                                                class="flex w-full pr-6 py-4 mb-2 justify-between items-center leading-7 rounded-2xl border-2 border-gray-200 bg-white p-2"
                                            >
                                                <summary
                                                    class="text-lg font-heading font-medium rounded-md"
                                                >
                                                    Caring
                                                </summary>
                                                <span
                                                    class="pl-6 text-justify border-t-2 border-gray-400"
                                                >
                                                    <%
                                                    if(animalData.caring)
                                                    { for(let care in
                                                    animalData.caring) {
                                                    %> 
                                                    <section class="flex flex-wrap">
                                                        <span class="w-3/12">
                                                            <%=
                                                            care.charAt(0).toUpperCase()
                                                            + care.slice(1) %>
                                                        </span>
                                                        <span class="w-9/12">
                                                            <%= animalData.caring[care] %>
                                                        </span>
                                                    </section>
                                                    <% } %> <% } else { %>
                                                    <span class="text-gray-400">
                                                        No information
                                                    </span>
                                                    <% } %>
                                                </span>
                                            </details>
                                        </div>
                                        <div class="flex flex-wrap">
                                            <details
                                                class="flex w-full pr-6 py-4 mb-2 justify-between items-center leading-7 rounded-2xl border-2 border-gray-200 bg-white p-2"
                                            >
                                                <summary
                                                    class="text-lg font-heading font-medium rounded-md"
                                                >
                                                    Health
                                                </summary>
                                                <span
                                                    class="pl-6 text-justify border-t-2 border-gray-400"
                                                >
                                                    <%
                                                    if(animalData.health)
                                                    { %>
                                                        <table class="text-inherit">
                                                    <% for(let disease of
                                                    animalData.health) {
                                                    %> 
                                                    <tr>
                                                        <td class="w-2/12 px-4 py-2">
                                                            <%=
                                                            disease.name.charAt(0).toUpperCase()
                                                            + disease.name.slice(1) %>
                                                        </td>
                                                        <td class="w-5/12 px-4 py-2">
                                                            <%=
                                                            disease.description %>
                                                        </td>
                                                        <td class="w-1/12 px-4 py-2">
                                                            <%=
                                                            disease.severity.charAt(0).toUpperCase()
                                                            + disease.severity.slice(1) %>
                                                        </td>
                                                        <td class="w-4/12 px-4 py-2">
                                                            <%=
                                                            disease.cause %>
                                                        </td>
                                                    </tr>
                                                        <% } %> 
                                                        </table>
                                                    <% } else { %>
                                                    <span class="text-gray-400">
                                                        No information
                                                    </span>
                                                    <% } %>
                                                </span>
                                            </details>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                </div>
            </div>
        </section>
        <!-- End Details -->

